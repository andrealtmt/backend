generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model participante {
  id             Int     @id @default(autoincrement())
  nombre         String
  apellidos      String
  email          String  @unique(map: "Participante_email_key")
  twitter        String
  ocupacion      String
  avatar         String
  aceptoTerminos Boolean

  @@index([nombre, apellidos], map: "Participante_nombre_apellidos_idx")
}
